  "jest": {
    "verbose": true,
    "setupFiles": [
      "<rootDir>/scripts/jest/setup.js"
    ],
    "testRegex": "\\.test\\.js$",
    "transformIgnorePatterns": [
      "node_modules/(?!bpk)"
    ],
    "testPathIgnorePatterns": [
      "backpack-react-native/",
      "node_modules",
      "backpack/"
    ],
    "moduleNameMapper": {
      "react-transition-group/CSSTransition": "<rootDir>/scripts/stubs/cssTransitionStub.js",
      "^.+\\.scss$": "<rootDir>/scripts/stubs/styleStub.js",
      ".(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|sketch|md|pdf)$": "<rootDir>/scripts/stubs/fileStub.js",
      "docs/src/pages/BagCheckPage/BagCheckPage.js": "<rootDir>/scripts/stubs/fileStub.js"
    },
    "coverageThreshold": {
      "global": {
        "statements": 75,
        "branches": 70,
        "functions": 75,
        "lines": 75
      }
    }
  },
  "greenkeeper": {
    "ignore": [
      "babel-loader",
      "copy-webpack-plugin",
      "css-loader",
      "danger",
      "extract-text-webpack-plugin",
      "file-loader",
      "json-loader",
      "postcss-loader",
      "raw-loader",
      "react",
      "react-dom",
      "react-test-renderer",
      "sass-loader",
      "style-loader",
      "webpack",
      "webpack-dev-server",
      "wrapper-webpack-plugin",
      "zip-it-loader"
    ]
  },
  "dependencies": {
    "get-contrast-ratio": "^0.2.1"
  }
